{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/Library/": true,
        "**/Temp/": true,
        "**/Obj/": true,
        "**/Build/": true,
        "**/Builds/": true,
        "**/Logs/": true,
        "**/UserSettings/": true,
        "**/.vs/": true,
        "**/*.csproj": true,
        "**/*.sln": true,
        
        "README.md": true,
        "**/AddressableAssetsData": true,
        "**/Assets/Plugins": true,
        "**/Packages": true,
        "**/.vscode": true,
        "**/ProjectSettings": true,
        "**.editorConfig": true,
    },
    "files.associations": {
        "*.asset": "yaml",
        "*.meta": "yaml",
        "*.prefab": "yaml",
        "*.unity": "yaml",
    },
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "*.sln": "*.csproj",
        "*.cs": "${capture}.*.cs, ${capture}.cs.meta",
        "*.shader": "${capture}.shader.meta",
    },
    "dotnet.defaultSolution": "Momentum.sln"
}